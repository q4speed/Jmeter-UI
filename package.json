{
  "name": "jmeter-ui",
  "version": "1.1.0",
  "main": "./lib/jmeter-ui.common.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-demo": "vue-cli-service build --mode demo --dest docs",
    "build": "vue-cli-service build --target lib --name jmeter-ui --dest lib ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "vue": "^2.6.11",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-unused-components": "off",
      "no-case-declarations": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-unused-expressions": "off",
      "no-unused-labels": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
